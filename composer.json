{
    "name": "devkindhq/simple-laravel-encryptable",
    "description": "",
    "keywords": [
        "devkind",
        "simple-encryptable",
        "simple-laravel-encryptable"
    ],
    "homepage": "https://github.com/devkindhq/simple-laravel-encryptable",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Saad Bhutto",
            "email": "saadbhutto@ymail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Devkind\\SimpleLaravelEncryptable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devkind\\SimpleLaravelEncryptable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devkind\\SimpleLaravelEncryptable\\SimpleLaravelEncryptableServiceProvider"
            ],
            "aliases": {
                "SimpleLaravelEncryptable": "Devkind\\SimpleLaravelEncryptable\\SimpleLaravelEncryptableFacade"
            }
        }
    }
}
